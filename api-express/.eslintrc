{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "standard",
  "plugins": [
  ],
  "rules": {
    "semi": ["error", "always"],
    "camelcase": 0,
    "no-trailing-spaces": "off",
    "indent": ["error", 4],
    "eol-last": "off",
    "prefer-promise-reject-errors": "off",
    "space-before-function-paren": "off",
    "object-curly-spacing": "off",
    "eqeqeq": "off",
    "no-eval": "off"
  }
}
