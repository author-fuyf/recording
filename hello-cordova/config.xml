<?xml version='1.0' encoding='utf-8'?>
<widget id="hello.cordova" version="1.0.0" version-time="2021/01/27 15:38:00" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloCordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <allow-navigation href="http://192.168.1.116:8080" />
        <allow-navigation href="https://portal.fuyunfeng.top/" />
        <allow-navigation href="https://landlord.fuyunfeng.top/" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <chcp>
        <config-file url="https://portal.fuyunfeng.top/hello-cordova/html-hot/chcp.json" />
        <auto-download enabled="false" />
        <auto-install enabled="false" />
    </chcp>
    <plugin name="cordova-hot-code-push-plugin" spec="^1.5.3" />
    <plugin name="cordova-plugin-device" spec="^2.0.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="^4.1.0" />
    <plugin name="cordova-plugin-screen-orientation" spec="^3.0.2" />
    <engine name="android" spec="^9.0.0" />
</widget>
